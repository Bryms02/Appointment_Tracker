<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="455.0" prefWidth="724.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view_controller.Main_Calendar_Controller">
    <children>
        
      <VBox layoutX="238.0" layoutY="125.0" prefHeight="224.0" prefWidth="437.0" spacing="45.0">
         <children>
            <TableView fx:id="apptTableview" prefHeight="230.0" prefWidth="437.0">
              <columns>
                <TableColumn fx:id="apptIdCol" prefWidth="75.0" text="Appt_ID" />
                <TableColumn fx:id="typeCol" prefWidth="75.0" text="Type" />
                <TableColumn fx:id="startCol" prefWidth="75.0" text="Start" />
                <TableColumn fx:id="endCol" prefWidth="81.0" text="End" />
                <TableColumn fx:id="customerIdCol" prefWidth="94.0" text="Patient_ID" />
              </columns>
            </TableView>
         </children></VBox>
      <Label layoutX="381.0" layoutY="98.0" text="This week's appointments">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button layoutX="587.0" layoutY="369.0" mnemonicParsing="false" onAction="#onSignOutButtonPushed" text="Sign Out" />
      <Button layoutX="55.0" layoutY="125.0" mnemonicParsing="false" onAction="#addCustomerButtonPushed" text="Patients">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Label layoutX="55.0" layoutY="28.0" text="Main Menu">
         <font>
            <Font name="System Bold Italic" size="25.0" />
         </font>
      </Label>
      <Button layoutX="55.0" layoutY="212.0" mnemonicParsing="false" onAction="#onAppointmentsButtonPushed" text="Appointments">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button layoutX="55.0" layoutY="305.0" mnemonicParsing="false" onAction="#onReportsButtonPushed" text="Reports">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
